<http://rdf.iit.demokritos.gr/2013/sevod> a owl:Ontology, adms:SemanticAsset;
    dct:title "Semagrow Vocabulary for Dataset Descriptions (Sevod)";
    dct:description "The Semagrow Vocabulary for Dataset Descriptions (Sevod) is an RDF Schema vocabulary for expressing metadata about RDF datasets, focusing on the kinds of metadata most useful for query optimization. This document provides a formal definition of the new RDF classes and properties introduced for Sevod. It is a companion to the main specification document for Sevod, <em><a href=\"http://rdf.iit.demokritos.gr/2014/sevod/doc\">Describing Datasets with the Sevod Vocabulary</a></em>.";
    dct:modified "2014-05-31"^^xsd:date;
    vann:preferredNamespaceUri "http://rdf.iit.demokritos.gr/2014/sevod#";
    vann:preferredNamespacePrefix "svd";
    foaf:homepage <http://rdf.iit.demokritos.gr/2014/sevod/doc>;
    dct:created "2013"^^xsd:date;
    dct:publisher "http://iit.demokritos.gr";
    dct:partOf <http://rdf.iit.demokritos.gr>;
    dct:type <http://purl.org/adms/assettype/Ontology>;
    dct:status <http://purl.org/adms/status/UnderDevelopment>;
    dc:creator
        <http://konstant.gr/stasinos>,
        <http://rdf.iit.demokritos.gr/2014/sevod#agcharal> .

